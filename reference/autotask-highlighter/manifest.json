{
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "*://*.autotask.net/*" ],
      "run_at": "document_idle"
   } ],
   "declarative_net_request": {
      "rule_resources": [ {
         "enabled": false,
         "id": "newTabRules",
         "path": "rulesets/newTabRules.json"
      } ]
   },
   "description": "Highlights the ticket timer a different colour depending on the elapsed time. Can also make Work Type field 'mandatory.",
   "host_permissions": [ "https://*.autotask.net/*" ],
   "icons": {
      "128": "images/logo128.png",
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo64.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Autotask Highlighter",
   "options_ui": {
      "open_in_tab": false,
      "page": "options.html"
   },
   "permissions": [ "storage", "declarativeContent", "declarativeNetRequest", "declarativeNetRequestWithHostAccess" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.1.0"
}
